{
    "id": "deezer-deezmate",
    "name": "Deezer (Deezmate)",
    "description": "Downloads FLAC from Deezer using the Deezmate API.",
    "type": "downloader",
    "author": "Kmax",
    "version": "0.9.2",
    "domains": [
        "deezer.com"
    ],
    "updateUrl": "https://raw.githubusercontent.com/royalashu4u/Kiokuu-extensions/main/deezer-deezmate.json",
    "capabilities": [
        "http"
    ],
    "headers": {
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36"
    },
    "download": {
        "resolver": {
            "url": "https://api.song.link/v1-alpha.1/links?url=https://open.spotify.com/track/{id}",
            "responseType": "json",
            "resultPath": "linksByPlatform.deezer.url",
            "regex": "/track/(\\d+)",
            "regexGroup": 1
        },
        "mirrorListUrl": "https://raw.githubusercontent.com/royalashu4u/Kiokuu-extensions/refs/heads/main/mirrors/mirrors.json",
        "mirrorPath": "deezer.mirrors",
        "mirrorUrlTemplate": "https://{domain}/dl/{id}",
        "responseType": "json",
        "urlPath": "links.FLAC"
    }
}