{
    "id": "tidal-qqdl",
    "name": "Tidal (QQDL Mirrors)",
    "description": "Downloads from Tidal using QQDL mirror APIs with Base64 manifest decoding.",
    "type": "downloader",
    "author": "Kmax",
    "version": "0.9.2",
    "domains": [
        "tidal.com"
    ],
    "updateUrl": "https://raw.githubusercontent.com/royalashu4u/Kiokuu-extensions/main/tidal-qqdl.json",
    "capabilities": [
        "http"
    ],
    "headers": {
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36"
    },
    "download": {
        "resolver": {
            "url": "https://api.song.link/v1-alpha.1/links?url=https://open.spotify.com/track/{id}",
            "responseType": "json",
            "resultPath": "linksByPlatform.tidal.url",
            "regex": "/track/(\\d+)",
            "regexGroup": 1
        },
        "mirrorListUrl": "https://raw.githubusercontent.com/royalashu4u/Kiokuu-extensions/refs/heads/main/mirrors/mirrors.json",
        "mirrorPath": "tidal.mirrors",
        "mirrorUrlTemplate": "https://{domain}/track/?id={id}&quality=LOSSLESS",
        "responseType": "json",
        "manifestPath": "data.manifest",
        "manifestUrlPath": "urls"
    }
}